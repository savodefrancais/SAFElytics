{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "functions": {
    "app.py": {
      "excludeFiles": "{.venv/**,__pycache__/**,.git/**,.pytest_cache/**,*.pyc,*.pyo}"
    }
  }
}
